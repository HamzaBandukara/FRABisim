C(c)=((c(d).[d#c]c<c>.D(c,d)))
D(c,d,b)=$a.(a<b>.0+a(b).E(c,d))
E(c,d)=(c<d>.E(c,d))+((c(e).[e#c][e#d]c<c>.F(c,d,e)))
F(c,d,e,b)=$a.(a<b>.0+a(b).G(c,d,e))
G(c,d,e)=(c<d>.G(c,d,e))+(c<e>.G(c,d,e))
J(c)=((c(d).[d#c]c<c>.K(c,d)))
K(c,d,b)=$a.(a<b>.0+a(b).L(c,d))
L(c,d)=(c<d>.L(c,d))+((c(e).[e#c][e#d]c<c>.M(c,d,e)))
M(c,d,e,b)=$a.(a<b>.0+a(b).N(c,d,e))
N(c,d,e)=(c<d>.N(c,d,e))+(c<e>.N(c,d,e))
TEST C(c) WITH J(c)