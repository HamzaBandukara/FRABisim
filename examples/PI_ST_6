AF(a,c,d,e) = ((a<e>.AE(a,c,d)) + (a(f).[f#a][f#c][f#d][f#e]a<a>.AG(a,c,d,e,f)))
AG(a,c,d,e,f) = ((a<f>.AF(a,c,d,e)) + (a(g).[g#a][g#c][g#d][g#e][g#f]a<a>.AH(a,c,d,e,f,g)))
