C(c) = ((c(d).[d#c]c<c>.D(c,d)))
D(c,d) = ((c<d>.C(c)))

G(c) = ((c(d).[d#c]c<c>.H(c,d)))
H(c,d) = ((c<d>.G(c)) + (c(e).[e#c][e#d]c<c>.I(c,d,e)))
I(c,d,e) = ((c<e>.H(c,d)) + (c(f).[f#c][f#d][f#e]c<c>.J(c,d,e,f)))
J(c,d,e,f) = ((c<f>.I(c,d,e)) + (c(g).[g#c][g#d][g#e][g#f]c<c>.K(c,d,e,f,g)))
K(c,d,e,f,g) = ((c<g>.J(c,d,e,f)))

TEST C(c) WITH G(c)