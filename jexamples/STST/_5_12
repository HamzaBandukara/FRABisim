C(c) = ((c(d).[d#c]c<c>.D(c,d)))
D(c,d) = ((c<d>.C(c)) + (c(e).[e#c][e#d]c<c>.E(c,d,e)))
E(c,d,e) = ((c<e>.D(c,d)) + (c(f).[f#c][f#d][f#e]c<c>.F(c,d,e,f)))
F(c,d,e,f) = ((c<f>.E(c,d,e)) + (c(g).[g#c][g#d][g#e][g#f]c<c>.G(c,d,e,f,g)))
G(c,d,e,f,g) = ((c<g>.F(c,d,e,f)) + (c(h).[h#c][h#d][h#e][h#f][h#g]c<c>.H(c,d,e,f,g,h)))
H(c,d,e,f,g,h) = ((c<h>.G(c,d,e,f,g)))

K(c) = ((c(d).[d#c]c<c>.L(c,d)))
L(c,d) = ((c<d>.K(c)) + (c(e).[e#c][e#d]c<c>.M(c,d,e)))
M(c,d,e) = ((c<e>.L(c,d)) + (c(f).[f#c][f#d][f#e]c<c>.N(c,d,e,f)))
N(c,d,e,f) = ((c<f>.M(c,d,e)) + (c(g).[g#c][g#d][g#e][g#f]c<c>.O(c,d,e,f,g)))
O(c,d,e,f,g) = ((c<g>.N(c,d,e,f)) + (c(h).[h#c][h#d][h#e][h#f][h#g]c<c>.P(c,d,e,f,g,h)))
P(c,d,e,f,g,h) = ((c<h>.O(c,d,e,f,g)) + (c(i).[i#c][i#d][i#e][i#f][i#g][i#h]c<c>.Q(c,d,e,f,g,h,i)))
Q(c,d,e,f,g,h,i) = ((c<i>.P(c,d,e,f,g,h)) + (c(j).[j#c][j#d][j#e][j#f][j#g][j#h][j#i]c<c>.R(c,d,e,f,g,h,i,j)))
R(c,d,e,f,g,h,i,j) = ((c<j>.Q(c,d,e,f,g,h,i)) + (c(k).[k#c][k#d][k#e][k#f][k#g][k#h][k#i][k#j]c<c>.S(c,d,e,f,g,h,i,j,k)))
S(c,d,e,f,g,h,i,j,k) = ((c<k>.R(c,d,e,f,g,h,i,j)) + (c(l).[l#c][l#d][l#e][l#f][l#g][l#h][l#i][l#j][l#k]c<c>.T(c,d,e,f,g,h,i,j,k,l)))
T(c,d,e,f,g,h,i,j,k,l) = ((c<l>.S(c,d,e,f,g,h,i,j,k)) + (c(m).[m#c][m#d][m#e][m#f][m#g][m#h][m#i][m#j][m#k][m#l]c<c>.U(c,d,e,f,g,h,i,j,k,l,m)))
U(c,d,e,f,g,h,i,j,k,l,m) = ((c<m>.T(c,d,e,f,g,h,i,j,k,l)) + (c(n).[n#c][n#d][n#e][n#f][n#g][n#h][n#i][n#j][n#k][n#l][n#m]c<c>.V(c,d,e,f,g,h,i,j,k,l,m,n)))
V(c,d,e,f,g,h,i,j,k,l,m,n) = ((c<n>.U(c,d,e,f,g,h,i,j,k,l,m)) + (c(o).[o#c][o#d][o#e][o#f][o#g][o#h][o#i][o#j][o#k][o#l][o#m][o#n]c<c>.W(c,d,e,f,g,h,i,j,k,l,m,n,o)))
W(c,d,e,f,g,h,i,j,k,l,m,n,o) = ((c<o>.V(c,d,e,f,g,h,i,j,k,l,m,n)))

TEST C(c) WITH K(c)