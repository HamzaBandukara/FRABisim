Q(a,b,c,d,e,f,g) = (a<b>.Q(a,b,c,d,e,f,g)) + (a<c>.Q(a,b,c,d,e,f,g)) + (a<d>.Q(a,b,c,d,e,f,g)) + (a<e>.Q(a,b,c,d,e,f,g)) + (a<f>.Q(a,b,c,d,e,f,g)) + (a<g>.Q(a,b,c,d,e,f,g)) + ((a(h).[h#a][h#b][h#c][h#d][h#e][h#f][h#g]a<a>.R(a,b,c,d,e,f,g,h)))
R(a,b,c,d,e,f,g,h) = (a<b>.R(a,b,c,d,e,f,g,h)) + (a<c>.R(a,b,c,d,e,f,g,h)) + (a<d>.R(a,b,c,d,e,f,g,h)) + (a<e>.R(a,b,c,d,e,f,g,h)) + (a<f>.R(a,b,c,d,e,f,g,h)) + (a<g>.R(a,b,c,d,e,f,g,h)) + (a<h>.R(a,b,c,d,e,f,g,h)) + ((a(i).[i#a][i#b][i#c][i#d][i#e][i#f][i#g][i#h]a<a>.S(a,b,c,d,e,f,g,h,i)))
S(a,b,c,d,e,f,g,h,i) = (a<b>.S(a,b,c,d,e,f,g,h,i)) + (a<c>.S(a,b,c,d,e,f,g,h,i)) + (a<d>.S(a,b,c,d,e,f,g,h,i)) + (a<e>.S(a,b,c,d,e,f,g,h,i)) + (a<f>.S(a,b,c,d,e,f,g,h,i)) + (a<g>.S(a,b,c,d,e,f,g,h,i)) + (a<h>.S(a,b,c,d,e,f,g,h,i)) + (a<i>.S(a,b,c,d,e,f,g,h,i)) + ((a(j).[j#a][j#b][j#c][j#d][j#e][j#f][j#g][j#h][j#i]a<a>.T(a,b,c,d,e,f,g,h,i,j)))
