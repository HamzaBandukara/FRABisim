C(c) = ((c(d).[d#c]c<c>.D(c,d)))
D(c,d) = ((c<d>.C(c)) + (c(e).[e#c][e#d]c<c>.E(c,d,e)))
E(c,d,e) = ((c<e>.D(c,d)) + (c(f).[f#c][f#d][f#e]c<c>.F(c,d,e,f)))
F(c,d,e,f) = ((c<f>.E(c,d,e)) + (c(g).[g#c][g#d][g#e][g#f]c<c>.G(c,d,e,f,g)))
G(c,d,e,f,g) = ((c<g>.F(c,d,e,f)) + (c(h).[h#c][h#d][h#e][h#f][h#g]c<c>.H(c,d,e,f,g,h)))
H(c,d,e,f,g,h) = ((c<h>.G(c,d,e,f,g)))

K(a) = ((a(b).[b#a]a<a>.L(a,b)))
L(a,b) = (a<b>.L(a,b)) + ((a(c).[c#a][c#b]a<a>.M(a,b,c)))
M(a,b,c) = (a<b>.M(a,b,c)) + (a<c>.M(a,b,c)) + ((a(d).[d#a][d#b][d#c]a<a>.N(a,b,c,d)))
N(a,b,c,d) = (a<b>.N(a,b,c,d)) + (a<c>.N(a,b,c,d)) + (a<d>.N(a,b,c,d)) + ((a(e).[e#a][e#b][e#c][e#d]a<a>.O(a,b,c,d,e)))
O(a,b,c,d,e) = (a<b>.O(a,b,c,d,e)) + (a<c>.O(a,b,c,d,e)) + (a<d>.O(a,b,c,d,e)) + (a<e>.O(a,b,c,d,e)) + ((a(f).[f#a][f#b][f#c][f#d][f#e]a<a>.P(a,b,c,d,e,f)))
P(a,b,c,d,e,f) = (a<b>.P(a,b,c,d,e,f)) + (a<c>.P(a,b,c,d,e,f)) + (a<d>.P(a,b,c,d,e,f)) + (a<e>.P(a,b,c,d,e,f)) + (a<f>.P(a,b,c,d,e,f)) + ((a(g).[g#a][g#b][g#c][g#d][g#e][g#f]a<a>.Q(a,b,c,d,e,f,g)))
Q(a,b,c,d,e,f,g) = (a<b>.Q(a,b,c,d,e,f,g)) + (a<c>.Q(a,b,c,d,e,f,g)) + (a<d>.Q(a,b,c,d,e,f,g)) + (a<e>.Q(a,b,c,d,e,f,g)) + (a<f>.Q(a,b,c,d,e,f,g)) + (a<g>.Q(a,b,c,d,e,f,g)) + ((a(h).[h#a][h#b][h#c][h#d][h#e][h#f][h#g]a<a>.R(a,b,c,d,e,f,g,h)))
R(a,b,c,d,e,f,g,h) = (a<b>.R(a,b,c,d,e,f,g,h)) + (a<c>.R(a,b,c,d,e,f,g,h)) + (a<d>.R(a,b,c,d,e,f,g,h)) + (a<e>.R(a,b,c,d,e,f,g,h)) + (a<f>.R(a,b,c,d,e,f,g,h)) + (a<g>.R(a,b,c,d,e,f,g,h)) + (a<h>.R(a,b,c,d,e,f,g,h)) + ((a(i).[i#a][i#b][i#c][i#d][i#e][i#f][i#g][i#h]a<a>.S(a,b,c,d,e,f,g,h,i)))
S(a,b,c,d,e,f,g,h,i) = (a<b>.S(a,b,c,d,e,f,g,h,i)) + (a<c>.S(a,b,c,d,e,f,g,h,i)) + (a<d>.S(a,b,c,d,e,f,g,h,i)) + (a<e>.S(a,b,c,d,e,f,g,h,i)) + (a<f>.S(a,b,c,d,e,f,g,h,i)) + (a<g>.S(a,b,c,d,e,f,g,h,i)) + (a<h>.S(a,b,c,d,e,f,g,h,i)) + (a<i>.S(a,b,c,d,e,f,g,h,i))

TEST C(c) WITH K(a)